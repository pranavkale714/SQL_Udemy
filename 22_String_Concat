-- 1. Concatenating Two Strings
SELECT CONCAT('Pranav','Kale');

-- 2. Concatenating Multiple Strings
SELECT CONCAT('Pranav', 'Dilip', 'Kale');

-- 3. Concatenating Strings with a Space
SELECT CONCAT('Pranak', ' ', 'Dilip', ' ', 'kale'); 

-- 4. Concatenating Columns
SELECT CONCAT(first_name, ' ', last_name) AS full_name FROM pranav;
